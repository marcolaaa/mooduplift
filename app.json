{
  "expo": {
    "name": "moodUplift",
    "slug": "mooduplift",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.marcolaaa.mooduplift"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [[
      "expo-font",
      {
        "fonts": ["./assets/fonts/Satisfy_Regular.ttf"]
      }]
    ]
  }
}
